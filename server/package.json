{
  "name": "server",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./index.js",
    "dbcreate": "npx sequelize db:create",
    "dbdrop": "npx sequelize db:drop",
    "mig": "npx sequelize db:migrate",
    "miguna": "npx sequelize db:migrate:undo:all",
    "seedall": "npx sequelize db:seed:all",
    "seeduna": "npx sequelize db:seed:undo:all",
    "dbinit": "npm run seedmongo && npm run cljson && npm run dbdrop && npm run dbcreate && npm run mig && npm run seedall",
    "cljson": "node ./src/utils/clearSequelizeJson.js",
    "seedmongo": "node ./src/utils/seedMongo.js",
    "lint": "eslint .",
    "fixlint": "eslint --fix .",
    "syncmodel": "node ./src/utils/syncModel.js",
    "syncallmodels": "node ./src/utils/syncAllModels.js",
    "dbcreate-test": "npx sequelize db:create --env test",
    "dbdrop-test": "npx sequelize db:drop --env test",
    "mig-test": "npx sequelize db:migrate --env test",
    "seedall-test": "npx sequelize db:seed:all --env test",
    "test": "npm run cljson && npm run dbdrop-test && npm run dbcreate-test && npm run mig-test && npm run seedall-test && jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sytnikovzp/home-accounting.git"
  },
  "bugs": {
    "url": "https://github.com/sytnikovzp/home-accounting/issues"
  },
  "homepage": "https://github.com/sytnikovzp/home-accounting#readme",
  "author": "Alexandr Sytnikov <sytnikov.zp@gmail.com>",
